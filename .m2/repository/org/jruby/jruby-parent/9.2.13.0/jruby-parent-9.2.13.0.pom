      <id>osgi</id>
      <build>
        <defaultGoal>install</defaultGoal>
        <plugins>
          <plugin>
            <artifactId>maven-invoker-plugin</artifactId>
            <configuration>
              <pomIncludes>
                <pomInclude>osgi*/pom.xml</pomInclude>
              </pomIncludes>
            </configuration>
          </plugin>
        </plugins>
      </build>
      <modules>
        <module>maven</module>
      </modules>
      <properties>
        <invoker.skip>false</invoker.skip>
        <its.osgi>no-excludes/pom.xml</its.osgi>
